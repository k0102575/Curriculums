package step21.ex2;

public class Sedan extends Car {
  boolean auto;
  Object storage;
  
  public Sedan(String model, String maker, int cc, boolean auto) {
    super(model, maker, cc);
    this.auto = auto;
  }
  
  public void load(Object obj) {
    this.storage = obj;
    System.out.printf("%s 를 실었습니다.", obj);
  }
  
  
}
