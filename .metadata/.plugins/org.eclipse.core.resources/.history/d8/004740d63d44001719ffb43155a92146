package step21.ex1;

public class Truck {
  String model;
  String maker;
  int cc;
  
  public Truck(String model, String maker, int cc) {
    this.model = model;
    this.maker = maker;
    this.cc = cc;
  }
  
  public void run() {
    System.out.println("부왕~~ 힘차게 달린다.");
  }
  
  public void load(Object obj) {
    System.out.printf("%s 를 실었습니다.", obj);
  }
  
  public void dump() {
    System.out.printf("%s 를 내렸씁니다.", onj);
  }
  
}
