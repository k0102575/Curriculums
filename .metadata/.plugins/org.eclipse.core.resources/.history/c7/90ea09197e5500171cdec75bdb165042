public class Test01 {
  public static void main(String[] args) {
    
    // 1) Spring 설정 파일 준비
    
    // 2) classpath 에 설정 파일이 있을 때
    ApplicationContext ctx = new ClassPathXmlApplicationContext("step27/ex1/application-context-01.xml");
    
  }
}
