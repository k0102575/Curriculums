package step21.ex1;

public class Bulldozer {
  String model;
  String maker;
  int cc;
  int bucketHeight;
  
  public Bulldozer(String model, String maker, int cc) {
    this.model = model;
    this.maker = maker;
    this.cc = cc;
  }
  
  public void run() {
    System.out.println("빠르렁~~ 덜덜덜 간다.");
  }
  
  
  
}
