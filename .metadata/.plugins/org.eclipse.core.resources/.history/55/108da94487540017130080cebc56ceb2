package bitcamp.java93.dao;

import java.util.List;

import bitcamp.java93.domain.Member;

public interface MemberDao {
  public List<Member> selectList(int pageNo, int pageSize) throws Exception; // selectList()

  public Member selectOne(int no) throws Exception; // selectOne()

  public int insert(Member member) throws Exception; // insert()

  public int delete(int no) throws Exception; // delete()

  public int update(Member member) throws Exception; // insert()

  public Member selectOneByEmailPassword(String email, String password) throws Exception; // selectOneByEmailPassword()

}
