/* 스프링 사용법: 팩토리 메서드 호출하기 III */

package step27.ex3;

import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

public class Test04 {
  public static void main(String[] args) {
    
    // 3) 스프링 IOC 컨테이너를 이용하여 팩토리 메서드를 사용하기

    ApplicationContext ctx = new ClassPathXmlApplicationContext("step27/ex3/application-context-03.xml");
    
    System.out.println(ctx.getBean("c1"));
    System.out.println(ctx.getBean("c2"));
    System.out.println(ctx.getBean("c3"));
    
  }
}
