package step21.ex1;

public class Truck {
  String model;
  String maker;
  int cc;
  boolean auto;
  
  public Truck(String model, String maker, int cc) {
    this.model = model;
    this.maker = maker;
    this.cc = cc;
  }
  
  public void run() {
    System.out.println("부웅~~ 부드럽게 달린다.");
  }
  
  public void load(Object obj) {
    System.out.printf("%s 를 실었습니다.", obj);
  }
  
  
}
